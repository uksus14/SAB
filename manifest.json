{
    "manifest_version": 3,
    "name": "SAB",
    "version": "1.0",
    "description": "Smart Address Bar",
    "permissions": [],
    "chrome_settings_overrides": {
      "search_provider": {
        "name": "SAB",
        "keyword": "sab",
        "favicon_url": "http://127.0.0.1:5000/favicon.ico",
        "search_url": "http://127.0.0.1:5000/?q={searchTerms}",
        "suggest_url": "http://127.0.0.1:5000/suggest?q={searchTerms}",
        "encoding": "UTF-8",
        "is_default": true
      }
    }
  }
  